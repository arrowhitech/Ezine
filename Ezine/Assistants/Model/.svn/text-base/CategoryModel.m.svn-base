#import "CategoryModel.h"

@implementation CategoryModel
@synthesize categoryId=_categoryId, sourceId=_sourceId , image=_image,title=_title;

-(id)initWithId:(NSInteger)cId andSourceId:(NSInteger)sId andImage:(NSString *)image andTitle:(NSString *)title{
    if (self=[super init]) {
        self.categoryId=cId;
        self.sourceId=sId;
        self.image=image;
        self.title=title;
    }
    return self;
}
@end
